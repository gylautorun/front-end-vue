{
    "name": "front-end-vue",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build:dev": "vue-tsc --noEmit && vite build",
        "build": "vue-tsc --noEmit && vite build --mode production && node scripts/config-version.js",
        "build:version": "node scripts/config-version.js",
        "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
        "eslint": "eslint \"src/**/*.{vue,ts,tsx}\"",
        "spell-check": "cspell \"src/**/*.*\"",
        "stylelint": "stylelint \"src/**/*.{vue,scss}\"",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "preview": "vite preview"
    },
